<p id="notice"><%= notice %></p>

<h1>Theorems</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @theorems.each do |theorem| %>
      <tr>
        <td><%= theorem.title %></td>
        <td><%= link_to 'Show', theorem %></td>
        <td><%= link_to 'Edit', edit_theorem_path(theorem) %></td>
        <td><%= link_to 'Destroy', theorem, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
